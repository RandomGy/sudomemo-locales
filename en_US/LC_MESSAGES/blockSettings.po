msgid "LOGIN_REQUIRED_TO_MANAGE_BLOCKS"
msgstr "Bạn cần phải đăng nhập để quản lý những người bạn đã chặn."

msgid "NO_USERS_BLOCKED"
msgstr "Bạn chưa chặn người nào."

msgid "BLOCKED_USERS"
msgstr "Những người đã chặn"

msgid "BLOCK_USER"
msgstr "Chặn người dùng"

msgid "COMMENT_BLOCK_IS_MUTUAL"
msgstr "Một khi bạn chặn người này, bạn sẽ không thể bình luận với nhau trên Flipnote."

msgid "USER_HAS_BEEN_BLOCKED"
msgstr "This user has been blocked."

msgid "IF_YOU_WISH_TO_UNBLOCK"
msgstr "Nếu bạn muốn bỏ chặn người này, thì bạn có thể vào phòng làm việc chọn những người đã chặn."

msgid "CONFIRM_BLOCK"
msgstr "Xác nhận chặn"

msgid "USER_ALREADY_BLOCKED"
msgstr "Bạn đã chặn người này rồi. Bạn có thể huỷ chặn người này trong mục Cài đặt nằm trong phòng làm việc, nhưng bạn phải đợi 24 tiếng trước khi có thể chặn lại."

msgid "ONE_DAY_WAIT_BEFORE_REBLOCK_FORMAT"
msgstr "Bạn đã chặn người này gần đây. Bạn phải đợi thêm %s"

msgid "UNBLOCK"
msgstr "Huỷ chặn"

msgid "LOGIN_REQUIRED"
msgstr "Bạn phải đăng nhập mới tiếp tục dược hành động này."

msgid "TAP_BACK_TO_RETURN"
msgstr "Chọn Trở về để quay lại phòng làm việc."

msgid "UNBLOCK_USER"
msgstr "Bỏ chặn người dùng"

msgid "CANNOT_UNBLOCK_NONBLOCKED_USER"
msgstr "Bạn không thể bỏ chặn người này, vì bạn chưa chặn người ấy."

msgid "USER_HAS_BEEN_UNBLOCKED"
msgstr "Bạn đã huỷ chặn người này."

msgid "COMMENTING_ENABLED_AFTER_UNBLOCK"
msgstr "Sau khi bạn đã huỷ chặn người này, họ có thể bình luận lại trên các Flipnote của bạn, trừ khi bạn cũng bị họ chặn."

msgid "UNBLOCK_WARNING_ONE_DAY_THRESHOLD"
msgstr "Nếu bạn huỷ chặn người này, bạn phải đợi 24 tiếng mới có thể chặn lại người này."

msgid "CONFIRM_UNBLOCK"
msgstr "Confirm Unblock"

msgid "RECENTLY_UNBLOCKED_ONE_DAY_THRESHOLD"
msgstr "Bạn gần đây đã huỷ chặn người này. Bạn phải đợi 24 tiếng kể từ lúc bạn huỷ chặn mới có thể chặn lại người này."
